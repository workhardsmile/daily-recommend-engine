= form_for @frequent_set do |f|
  - if @frequent_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@frequent_set.errors.count, "error")} prohibited this frequent_set from being saved:"
      %ul
        - @frequent_set.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :surport_level
    = f.number_field :surport_level
  .field
    = f.label :last_sn
    = f.text_field :last_sn
  .field
    = f.label :type
    = f.text_field :type
  .field
    = f.label :date_label
    = f.text_field :date_label
  .actions
    = f.submit 'Save'
